S_0x7fb450604180 .scope module, "simple_counter_tb" "simple_counter_tb" 2 6;
 .timescale 0 0;
v0x7fb4506147a0_0 .var "clk", 0 0;
v0x7fb450614840_0 .var "reset", 0 0;
v0x7fb4506148f0_0 .net "value", 31 0, v0x7fb450614690_0;  1 drivers
S_0x7fb450604300 .scope module, "c1" "simple_counter" 2 19, 2 26 0, S_0x7fb450604180;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 32 "value";
v0x7fb450604550_0 .net "clk", 0 0, v0x7fb4506147a0_0;  1 drivers
v0x7fb4506145f0_0 .net "reset", 0 0, v0x7fb450614840_0;  1 drivers
v0x7fb450614690_0 .var "value", 31 0;
E_0x7fb450604510 .event posedge, v0x7fb4506145f0_0, v0x7fb450604550_0;
    .scope S_0x7fb450604300;
T_0 ;
    %wait E_0x7fb450604510;
    %load/vec4 v0x7fb4506145f0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x7fb450614690_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fb450614690_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x7fb450614690_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fb450604180;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fb4506147a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fb450614840_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x7fb450604180;
T_2 ;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fb450614840_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fb450614840_0, 0, 1;
    %delay 400, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x7fb450604180;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x7fb4506147a0_0;
    %nor/r;
    %store/vec4 v0x7fb4506147a0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x7fb450604180;
T_4 ;
    %vpi_call 2 22 "$monitor", "Time: %t, Value: %d", $time, v0x7fb4506148f0_0 {0 0 0};
    %end;
    .thread T_4;
