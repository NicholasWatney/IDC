S_0x7fd33850e0d0 .scope module, "read_file" "read_file" 2 6;
 .timescale 0 0;
v0x7fd338507360_0 .var/s "captured_data", 21 0;
v0x7fd339107080_0 .var "clk", 0 0;
v0x7fd339107130_0 .var "count", 63 1;
v0x7fd3391071f0_0 .var/i "data_file", 31 0;
v0x7fd3391072a0_0 .var/i "scan_file", 31 0;
E_0x7fd33850dde0 .event posedge, v0x7fd339107080_0;
    .scope S_0x7fd33850e0d0;
T_0 ;
    %pushi/vec4 0, 0, 63;
    %store/vec4 v0x7fd339107130_0, 0, 63;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fd339107080_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x7fd33850e0d0;
T_1 ;
    %vpi_call 2 14 "$write", "Starting simulation.\012" {0 0 0};
    %vpi_func 2 15 "$fopen" 32, "box_inputs.txt", "r" {0 0 0};
    %store/vec4 v0x7fd3391071f0_0, 0, 32;
    %load/vec4 v0x7fd3391071f0_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 17 "$write", "Error in opening file.\012" {0 0 0};
    %vpi_call 2 18 "$finish" {0 0 0};
T_1.0 ;
    %vpi_call 2 20 "$write", "Successfully opened file.\012" {0 0 0};
    %delay 500, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x7fd33850e0d0;
T_2 ;
    %delay 1, 0;
    %load/vec4 v0x7fd339107080_0;
    %nor/r;
    %store/vec4 v0x7fd339107080_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x7fd33850e0d0;
T_3 ;
    %wait E_0x7fd33850dde0;
    %load/vec4 v0x7fd339107130_0;
    %addi 1, 0, 63;
    %store/vec4 v0x7fd339107130_0, 0, 63;
    %vpi_call 2 28 "$write", "Clock: %d ", v0x7fd339107130_0 {0 0 0};
    %vpi_func 2 29 "$fscanf" 32, v0x7fd3391071f0_0, "%d\012", v0x7fd338507360_0 {0 0 0};
    %store/vec4 v0x7fd3391072a0_0, 0, 32;
    %vpi_func 2 30 "$feof" 32, v0x7fd3391071f0_0 {0 0 0};
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %vpi_call 2 31 "$write", "Value: %d\012", v0x7fd338507360_0 {0 0 0};
    %jmp T_3.1;
T_3.0 ;
    %vpi_call 2 33 "$write", "\012" {0 0 0};
T_3.1 ;
    %jmp T_3;
    .thread T_3;
